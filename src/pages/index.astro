---
import Layout from "@layouts/Layout.astro";
import { $counter } from "@store/store";
import ListUsers from "@components/ListUsers.astro";

$counter.set(await Astro.glob("@json/*.json"));
---

<Layout title="CV List">
  <main class="text-center flex flex-col gap-6 py-10">
    <h1 class="text-yellow-500 text-2xl">CV List</h1>
    <ListUsers />
  </main>
</Layout>
